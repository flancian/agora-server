{
    "python.pythonPath": "venv/bin/python",
	"files.watcherExclude": {
		"**/.git/objects/**": true,
		"**/.git/subtree-cache/**": true,
		"**/node_modules/*/**": true,
		"**/venv/*/**": true
},
"[python]": {
	"editor.defaultFormatter": "ms-python.black-formatter"
},
"python.formatting.provider": "none"
}
